// n adet girilen 5 sayinin ortalamasini alan program.

 import java.util.*;
public class Main {
	public static void main(String [] args) {
		Scanner giris = new Scanner(System.in);
		    int n = 5;
			int toplam = 0;
			System.out.println("Sayilari giriniz:");
			for (int i=0;i<n;i++) {
				toplam += giris.nextInt();
			}
			int ortalama = toplam/n;
			System.out.println("Sayilarin toplami:");
			System.out.println(toplam);
		}
}

//Verilen dizideki en büyük k adet sayıyı bulan program.

import java.util.*;
public class main {
	
    public static void main(String[] args) 
    {
        Integer sayilar[] = new Integer[]{1, 4, 17, 7, 25, 3, 100};
        int k = 3;
		System.out.println("Sayilar: ");
		System.out.println(Arrays.toString(sayilar));
		System.out.println(k +" en büyük sayisi:");
        Arrays.sort(sayilar, Collections.reverseOrder());
       for (int i = 0; i < k; i++) 
          System.out.print(sayilar[i] + " ");
      }      
  }
